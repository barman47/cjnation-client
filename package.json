{
  "name": "cjnation-server",
  "version": "1.0.0",
  "description": "CJ Nation Server ",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:build": "tsc -w",
    "start:run": "cross-env NODE_ENV=development nodemon build/server.js",
    "start": "cross-env NODE_ENV=production node build/server.js",
    "server": "ts-node-dev --respawn --pretty --transpile-only --clear src/server.ts",
    "test": "jest --verbose",
    "test-watch": "NODE_ENV=test jest --watch --detectOpenHandles --timeout=10000",
    "test-coverage": "npm test -- --coverage --collectCoverageFrom=\"./src/**\"",
    "seed": "ts-node-dev --respawn --pretty --transpile-only --clear src/data/seed.ts"
  },
  "keywords": [
    "NodeJS",
    "express",
    "Typescript",
    "Mongoose",
    "MongoDB"
  ],
  "author": "Uzoanya Dominic",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^7.6.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-force-https": "^1.0.0",
    "form-data": "^4.0.0",
    "google-auth-library": "^9.2.0",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^9.3.0",
    "mongoose": "^8.0.1",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "resend": "^2.0.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.4.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/validator": "^13.11.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": "18.18.2",
    "npm": "9.8.1"
  }
}
